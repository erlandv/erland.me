---
import Icon from "./Icon.astro";

interface Props {
  href: string;
  label?: string;
}

const { href, label = 'Back' } = Astro.props as Props;
---

<div class="back-nav-container">
  <a href={href} class="back-nav-button w-inline-block">
    <Icon name="back" class="back-nav-img" />
    <div>{label}</div>
  </a>
  
</div>

